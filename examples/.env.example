# Kamiwaza API configuration

# Single API URL
KAMIWAZA_API_URL=https://localhost/api

# Multiple API URLs for testing with multiple instances
KAMIWAZA_TEST_URL_LIST=https://localhost/api,https://prod.kamiwaza.ai/api

# Pattern to filter models (e.g., "qwen", "gemma", "static")
KAMIWAZA_MODEL_PATTERN=qwen

# SSL verification setting
KAMIWAZA_VERIFY_SSL=false
